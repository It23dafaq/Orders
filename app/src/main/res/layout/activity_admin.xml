<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".UI.AdminActivity">

    <ImageView
            android:layout_width="156dp"
            android:layout_height="174dp" app:srcCompat="@android:drawable/ic_menu_month"
            android:id="@+id/imageView7"
            android:layout_marginTop="16dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@+id/Daily" android:layout_marginStart="15dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
    />
    <ImageView
            android:layout_width="156dp"
            android:layout_height="174dp" app:srcCompat="@android:drawable/ic_menu_day"
            android:id="@+id/Daily"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="20dp"
            android:layout_marginTop="15dp" app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="Daily Orders"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9" app:layout_constraintStart_toStartOf="@+id/Daily"
            app:layout_constraintBottom_toBottomOf="@+id/Daily"
            android:textSize="20sp"/>
    <TextView
            android:text="Monthly Orders"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11" app:layout_constraintStart_toStartOf="@+id/imageView7"
            app:layout_constraintBottom_toBottomOf="@+id/imageView7" android:textSize="20sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>